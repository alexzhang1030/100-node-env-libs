{
  "name": "100-node-env-libs",
  "private": true,
  "license": "MIT",
  "author": "alexzhang1030",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexzhang1030/100-node-env-libs.git"
  },
  "scripts": {
    "stub": "unbuild --stub",
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build",
    "release": "bumpp --commit --push --tag && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@developer-plus/eslint-config": "^0.1.1",
    "@types/node": "^18.0.1",
    "bumpp": "^8.2.1",
    "eslint": "^8.19.0",
    "esno": "^0.16.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "pnpm": "^7.5.0",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.4",
    "unplugin-auto-import": "^0.7.2",
    "vitest": "^0.17.0"
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
